#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    int t;
    cin>>t;
    while(t--){
        char a;
    set<char> s;
    for (int i = 0; i < 4; i++)
    {
        cin >> a;
        s.insert(a);
    }
    cout << s.size() - 1 << endl;
    }

    return 0;
}
